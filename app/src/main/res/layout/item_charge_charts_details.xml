<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/item_details_title"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:text="曲靖南兴花园金穗花园"
        android:textSize="@dimen/_16sp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:textColor="@color/black"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/item_details_location"
        app:layout_constraintTop_toBottomOf="@id/item_details_title"
        android:layout_width="0dp"
        app:layout_constraintLeft_toLeftOf="@id/item_details_title"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="15dp"
        android:text="昆明市-五华区"
        android:drawablePadding="5dp"
        android:textSize="@dimen/_12sp"
        android:drawableStart="@mipmap/inspect_location"
        android:layout_height="wrap_content"/>

    <ImageView
        android:id="@+id/icon1"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/item_details_location"
        app:layout_constraintLeft_toLeftOf="parent"
        android:src="@drawable/charge_details1"
        android:layout_marginTop="10dp"
        android:layout_marginStart="15dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/icon1_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_11sp"
        android:textColor="@color/black"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toTopOf="@id/icon1"
        app:layout_constraintLeft_toRightOf="@+id/icon1"
        android:text="有"/>

    <TextView
        android:id="@+id/icon1_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_11sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/icon1_text"
        app:layout_constraintLeft_toLeftOf="@id/icon1_text"
        android:text="三维信息"/>



    <ImageView
        android:id="@+id/icon2"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/icon1_text"
        app:layout_constraintTop_toTopOf="@id/icon1_text"
        android:src="@drawable/charge_details2"
        android:layout_marginStart="80dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/icon2_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_11sp"
        android:textColor="@color/black"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toTopOf="@id/icon2"
        app:layout_constraintLeft_toRightOf="@+id/icon2"
        android:text="已接管"/>

    <TextView
        android:id="@+id/icon2_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_11sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/icon2_text"
        app:layout_constraintLeft_toLeftOf="@id/icon2_text"
        android:text="消防接管状态"/>


    <ImageView
        android:id="@+id/icon3"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/icon2_text"
        app:layout_constraintTop_toTopOf="@id/icon2_text"
        android:src="@drawable/charge_details3"
        android:layout_marginStart="80dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/icon3_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_11sp"
        android:textColor="@color/black"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toTopOf="@id/icon3"
        app:layout_constraintLeft_toRightOf="@+id/icon3"
        android:text="正常"/>

    <TextView
        android:id="@+id/icon3_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/_11sp"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/icon3_text"
        app:layout_constraintLeft_toLeftOf="@id/icon3_text"
        android:text="通讯状态"/>
    <ImageView
        android:id="@+id/icon4"
        app:layout_constraintTop_toBottomOf="@id/icon1_desc"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="wrap_content"
        android:src="@drawable/charge_details4"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/charge_project"
        android:layout_width="@dimen/charge_charts_details_text_width"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icon4"
        app:layout_constraintLeft_toRightOf="@id/icon4"
        android:text="直管单位"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/icon4_text"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/charge_project"
        app:layout_constraintLeft_toRightOf="@id/charge_project"
        android:text="消防总队"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/text_01"
        android:layout_width="@dimen/charge_charts_details_text_width"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icon4_text"
        app:layout_constraintLeft_toRightOf="@id/icon4_text"
        android:text="联系方式"
        android:layout_marginStart="15dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/text_011"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/text_01"
        app:layout_constraintLeft_toRightOf="@id/text_01"
        android:text="19898008790"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <ImageView
        android:id="@+id/icon5"
        app:layout_constraintTop_toBottomOf="@id/icon4"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="wrap_content"
        android:src="@drawable/charge_details5"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/text_02"
        android:layout_width="@dimen/charge_charts_details_text_width"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icon5"
        app:layout_constraintLeft_toRightOf="@id/icon5"
        android:text="业主"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/icon5_text"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/text_02"
        app:layout_constraintLeft_toRightOf="@id/text_02"
        android:text="消防总队"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/text_03"
        android:layout_width="@dimen/charge_charts_details_text_width"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icon5_text"
        app:layout_constraintLeft_toRightOf="@id/icon5_text"
        android:text="联系方式"
        android:layout_marginStart="15dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/text_012"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/text_03"
        app:layout_constraintLeft_toRightOf="@id/text_03"
        android:text="19898008790"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>


    <ImageView
        android:id="@+id/icon6"
        app:layout_constraintTop_toBottomOf="@id/icon5"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="wrap_content"
        android:src="@drawable/charge_details6"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/text_04"
        android:layout_width="@dimen/charge_charts_details_text_width"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icon6"
        app:layout_constraintLeft_toRightOf="@id/icon6"
        android:text="维保状态"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/icon6_text"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/text_04"
        app:layout_constraintLeft_toRightOf="@id/text_04"
        android:text="消防总队"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/text_05"
        android:layout_width="@dimen/charge_charts_details_text_width"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icon6_text"
        app:layout_constraintLeft_toRightOf="@id/icon6_text"
        android:text="维保单位"
        android:layout_marginStart="15dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/text_013"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/text_05"
        app:layout_constraintLeft_toRightOf="@id/text_05"
        android:text="19898008790"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>


    <TextView
        android:id="@+id/text_07"
        android:layout_width="@dimen/charge_charts_details_text_width"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/text_04"
        app:layout_constraintLeft_toLeftOf="@id/text_04"
        android:text="联系人"
        android:layout_marginTop="3dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/icon8_text"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/text_07"
        app:layout_constraintLeft_toRightOf="@id/text_07"
        android:text="消防总队"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/text_09"
        android:layout_width="@dimen/charge_charts_details_text_width"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icon8_text"
        app:layout_constraintLeft_toRightOf="@id/icon8_text"
        android:text="联系方式"
        android:layout_marginStart="15dp"
        android:textSize="@dimen/_11sp"/>

    <TextView
        android:id="@+id/text_014"
        android:textColor="@color/black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/text_09"
        app:layout_constraintLeft_toRightOf="@id/text_09"
        android:text="19898008790"
        android:layout_marginStart="5dp"
        android:textSize="@dimen/_11sp"/>

    <View
        app:layout_constraintTop_toBottomOf="@id/text_07"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@color/menu_normal_bac"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginTop="15dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>